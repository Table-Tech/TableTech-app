    "@mollie/api-client": "^4.0.0",
    "@prisma/client": "^6.13.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "bcryptjs": "^3.0.2",
    "fastify": "^5.3.3",
    "fastify-plugin": "^5.0.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^5.6.1",
    "socket.io": "^4.8.1",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.15.18",
